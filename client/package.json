{
  "name": "mailtrain-client",
  "version": "2.0.0",
  "description": "Self hosted email newsletter app - client components",
  "main": "index.js",
  "scripts": {
    "setdate": "sed -i.bak -E \"s/__BUILD_DATE__/`date -I`/g\" src/Home.js",
    "js": "webpack",
    "watch-js": "webpack --watch -d inline-source-map",
    "css": "npm-run-all --sequential css-compile css-minify",
    "css-compile": "npx sass --source-map src/scss/mailtrain.scss dist/mailtrain.css",
    "css-minify": "cleancss --source-map --source-map-inline-sources --input-source-map dist/mailtrain.css.map --output dist/mailtrain.min.css dist/mailtrain.css",
    "watch-css": "nodemon --watch src/scss -e scss -x \"npm run css\"",
    "watch": "npx npm-run-all --parallel watch-css watch-js",
    "build": "npm-run-all --parallel css js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/highcanfly-club/hcfmailer-plus.git"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/highcanfly-club/hcfmailer-plus",
  "dependencies": {
    "@coreui/coreui": "^4.3.2",
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.7.9",
    "bootstrap": "^5.3.3",
    "clone": "^2.1.2",
    "datatables.net": "^2.2.0",
    "datatables.net-bs4": "^2.2.0",
    "date-fns": "^2.30.0",
    "ellipsize": "^0.5.1",
    "grapesjs": "^0.22.4",
    "grapesjs-mjml": "^1.0.6",
    "grapesjs-preset-newsletter": "^1.0.2",
    "htmlparser2": "^10.0.0",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "immutable": "^4.2.4",
    "juice": "^11.0.0",
    "lodash": "^4.17.21",
    "mjml4-in-browser": "^1.1.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.6",
    "prop-types": "^15.8.1",
    "querystringify": "^2.2.0",
    "react": "^18.3.0",
    "react-ace": "^10.1.0",
