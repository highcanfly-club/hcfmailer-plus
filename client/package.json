{
  "name": "mailtrain-client",
  "version": "2.0.0",
  "description": "Self hosted email newsletter app - client components",
  "main": "index.js",
  "scripts": {
    "setdate": "sed -i.bak -E \"s/__BUILD_DATE__/`date -I`/g\" src/Home.js" ,
    "js": "webpack",
    "watch-js": "webpack --watch",
    "css": "npm-run-all --sequential css-compile css-minify",
    "css-compile": "npx sass --source-map src/scss/mailtrain.scss dist/mailtrain.css",
    "css-minify": "cleancss --source-map --source-map-inline-sources --input-source-map dist/mailtrain.css.map --output dist/mailtrain.min.css dist/mailtrain.css",
    "watch-css": "nodemon --watch src/scss -e scss -x \"npm run css\"",
    "watch": "npm-run-all --parallel watch-css watch-js",
    "build": "npm-run-all --parallel css js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Mailtrain-org/mailtrain.git"
  },
  "license": "GPL-3.0",
  "homepage": "https://mailtrain.org/",
  "dependencies": {
    "@coreui/coreui": "^2.1.16",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.6.2",
    "clone": "^2.1.2",
    "datatables.net": "^1.10.24",
    "datatables.net-bs4": "^1.10.24",
    "ellipsize": "^0.1.0",
    "grapesjs": "^0.20.4",
    "grapesjs-mjml": "^1.0.4",
    "grapesjs-preset-newsletter": "^1.0.1",
    "htmlparser2": "^3.10.1",
    "i18next": "^19.9.2",
    "i18next-browser-languagedetector": "^4.3.1",
    "immutable": "^4.2.4",
    "juice": "^6.0.0",
    "lodash": "^4.17.21",
    "mjml4-in-browser": "^1.1.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "popper.js": "^1.16.1",
    "prop-types": "^15.8.1",
    "querystringify": "^2.2.0",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-ckeditor-component": "^1.1.0",
    "react-color": "^2.19.3",
